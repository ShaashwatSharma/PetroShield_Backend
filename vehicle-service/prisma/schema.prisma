generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Vehicle {
  id             String   @id @default(uuid())
  registration   String   @unique
  organizationId String
  createdAt      DateTime @default(now())
}

model Sensor {
  id         String   @id @default(uuid())
  sensorId   String   @unique
  vehicleId  String
  location   String?
}
